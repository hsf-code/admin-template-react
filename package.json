    "antd": "^5.3.0",
    "axios": "^1.3.4",
    "driver.js": "^0.9.8",
    "echarts": "^5.4.1",
    "echarts-liquidfill": "^3.1.0",
    "i18next": "^22.4.11",
    "immer": "^9.0.19",
    "js-md5": "^0.7.3",
    "moment": "^2.29.4",
    "nprogress": "^0.2.0",
    "qs": "^6.11.1",
    "react": "^18.2.0",
    "react-activation": "^0.12.2",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "react-transition-group": "^4.4.5",
    "redux": "^4.2.1",
